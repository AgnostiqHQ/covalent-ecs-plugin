{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BraketAccess",
            "Effect": "Allow",
            "Action": "braket:*",
            "Resource": "*"
        },
        {
            "Sid": "ObjectStore",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::<s3_resource_bucket>/*",
                "arn:aws:s3:::<s3_resource_bucket>"
            ]
        }
    ]
}
